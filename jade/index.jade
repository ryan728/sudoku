html
 head
  title   Sudoku
  link(rel='stylesheet', type='text/css', href='css/sudoku.css', media='screen')
  script
   var cheatsheet = '#{cheatsheet}'
   var puzzle = '#{puzzle}'
  script(type='text/javascript', src='lib/jquery-1.6.1.js')
  script(type='text/javascript', src='lib/cake.js')
  script(type='text/javascript', src='client/cell.js')
  script(type='text/javascript', src='client/sudoku.js')
  script(type='text/javascript', src='lib/socket.io.js')
 body
  section
   label(for='nick') Name:
   input#nickInput(type='text', name='nick')
   input#connectBtn(type='submit', value='Join')
  section
   canvas#puzzle_canvas(width='450', height='450')

  table#number_table(border='1px')
   tr
    td
     div.number_1
    td
     div.number_2
    td
     div.number_3
    td
     div.number_4
    td
     div.number_5
    td
     div.number_6
    td
     div.number_7
    td
     div.number_8
    td
     div.number_9